{
  "name": "annotinder-client",
  "homepage": "https://ccs-amsterdam.github.io/annotinder-client",
  "description": "The AnnoTinder React Client",
  "author": "Kasper Welbers",
  "keywords": [
    "react",
    "annotation"
  ],
  "version": "0.3.14",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccs-amsterdam/annotinder-client"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@tanstack/react-query": "^4.10.1",
    "axios": "^1.0.0",
    "compromise": "^14.5.0",
    "copyfiles": "^2.4.1",
    "fomantic-ui": "^2.9.0",
    "fomantic-ui-css": "^2.9.0",
    "gh-pages": "^4.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "qrcode.react": "^3.1.0",
    "randomcolor": "^0.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-full-screen": "^1.1.1",
    "react-markdown": "^8.0.3",
    "react-papaparse": "^4.1.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "react-swipeable": "^7.0.0",
    "rimraf": "^3.0.2",
    "semantic-ui-react": "^2.1.3",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.10.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.64",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "react-scripts build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!compromise)/\"",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "copy-files": "copyfiles -u 2 src/lib/**/*.html src/lib/**/*.css dist/",
    "build_npm": "rimraf dist/ && tsc && npm run copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
