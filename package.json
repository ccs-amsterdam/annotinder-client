{
  "name": "ccs-annotator-client",
  "homepage": "https://ccs-amsterdam.github.io/ccs-annotator-client",
  "description": "The main workhorse behind the CCS Annotator",
  "author": "Kasper Welbers",
  "keywords": [
    "react",
    "annotation"
  ],
  "version": "0.2.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccs-amsterdam/ccs-annotator-client"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@semantic-ui-react/css-patch": "^1.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.25.0",
    "compromise": "^13.11.4",
    "compromise-paragraphs": "^0.0.5",
    "qrcode.react": "^3.0.1",
    "randomcolor": "^0.6.2",
    "react": "^18.0.0",
    "react-dom": "^17.0.2",
    "react-full-screen": "^1.1.0",
    "react-papaparse": "^4.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-swipeable": "^6.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "react-scripts build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build_npm": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "postinstall": "semantic-ui-css-patch"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.17.9",
    "babel-jest": "^28.0.2",
    "gh-pages": "^3.1.0",
    "jest": "^28.0.2"
  }
}
